.vscode/
.idea/
*.swp
*.swo

# Python virtual environment
neural-rink-env/
venv/
env/

# Python cache
__pycache__/
*.py[cod]
*$py.class

# Training results (keep model files, ignore temp)
results/*/events.out.tfevents.*
results/*/checkpoint
results/*/tb_logs/
results/*/tensorboard_logs/

